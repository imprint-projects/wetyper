{
    "name": "wetyper/wetyper",
    "type": "library",
    "description": "WeTyper is an amazing and powerful open source blog system.",
    "keywords": [
        "wetyper",
        "blog"
    ],
    "license": "MIT",
    "homepage": "https://github.com/wetyper/wetyper",
    "support": {
        "issues": "https://github.com/wetyper/wetyper/issues",
        "source": "https://github.com/wetyper/wetyper"
    },
    "authors": [
        {
            "name": "You Ming",
            "email": "youming@funcuter.org"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "WeTyper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WeTyper\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
